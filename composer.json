{
    "name": "yoeunes/notify-laravel",
    "description": "Laravel Notify package for yoeunes/notify",
    "keywords": [
        "yoeunes",
        "notify",
        "php",
        "laravel",
        "symfony",
        "Lumen",
        "notifications",
        "messages",
        "alerts",
        "pnotify",
        "toastr ",
        "bundle",
        "flex"
    ],
    "homepage": "https://github.com/yoeunes/notify-laravel",
    "authors": [
        {
            "name": "Younes Khoubza",
            "email": "younes.khoubza@gmail.com",
            "homepage": "https://github.com/yoeunes",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "yoeunes/notify": "v2.x-dev",
        "illuminate/support": "^5.0 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7 || ^6.0 || ^7.0 || ^8.3",
        "orchestra/testbench": "^3.0 || ^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Yoeunes\\Notify\\Laravel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yoeunes\\Notify\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yoeunes\\Notify\\Laravel\\NotifyServiceProvider"
            ],
            "aliases": {
                "Notify": "Yoeunes\\Notify\\Laravel\\Facades\\Notify"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
